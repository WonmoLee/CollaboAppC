{
  "name": "collaboappc",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "electron-builder build",
    "deploy:win64": "electron-builder --win nsis:x64"
  },
  "dependencies": {
    "axios": "~0.18.0",
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.7.0",
    "electron-packager": "^15.5.1",
    "socket.io-client": "~2.1.1"
  },
  "author": "wonmoLee",
  "license": "ISC",
  "build": {
    "productName": "OM",
    "appId": "com.Collabo.electron",
    "asar": true,
    "mac": {
      "target": [
        "default"
      ],
      "icon": ""
    },
    "dmg": {
      "title": "OM",
      "icon": ""
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": ""
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "perMachine": true,
      "language": 1042,
      "installerIcon": "",
      "shortcutName": "OM",
      "artifactName": "OM-${version}.${ext}",
      "uninstallDisplayName": "OM"
    },
    "directories": {
      "buildResources": "./build/",
      "output": "./dist/"
    }
  },
  "devDependencies": {
    "electron": "~2.0.3",
    "electron-builder": "^23.0.3",
    "electron-winstaller": "^5.0.0",
    "webpack": "^5.73.0"
  }
}
